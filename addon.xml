<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="skin.confluence_xbmb3c"
  version="0.2.15"
  name="Confluence XBMB3C"
  provider-name="null_pointer">
  <requires>
    <import addon="xbmc.gui" version="5.0.0"/>
  </requires>
  <extension
    point="xbmc.gui.skin"
    defaultthemename="textures.xbt"
    debugging="false"
    effectslowdown="0.75">
    <res width="1280" height="720" aspect="16:9" default="true" folder="720p" />
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Xbmb3c Confluence skin</summary>
    <description lang="en">A Media Browser Server compatible skin</description>
    <disclaimer lang="en">Based on the default Confluence Skin</disclaimer>
    <platform>all</platform>
  </extension>
</addon>
